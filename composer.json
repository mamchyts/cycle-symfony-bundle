{
    "name": "mamchyts/cycle-symfony-bundle",
    "description": "Cycle ORM symfony support",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Dmitry Mamchyts"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "cycle/annotated": "^3",
        "cycle/migrations": "^4",
        "cycle/orm": "^2",
        "cycle/schema-builder": "^2",
        "cycle/schema-migrations-generator": "^3",
        "symfony/config": "^6.1",
        "symfony/dependency-injection": "^6.1",
        "symfony/http-kernel": "^6.1"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.28",
        "friendsofphp/php-cs-fixer": "^3.11",
        "phpstan/phpstan": "^1.8.8",
        "phpstan/phpstan-doctrine": "^1.3.16",
        "phpstan/phpstan-strict-rules": "^1.4.4",
        "phpunit/php-code-coverage": "^9.2.17"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Cycle\\SymfonyBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
