{
    "name": "mamchyts/cycle-symfony-bundle",
    "description": "Cycle ORM symfony support",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Dmitry Mamchyts"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "cycle/annotated": "^3.2",
        "cycle/migrations": "^3.1",
        "cycle/orm": "^2.2",
        "cycle/schema-builder": "^2.1",
        "cycle/schema-migrations-generator": "^2",
        "doctrine/collections": "^2.0",
        "symfony/config": "^6.1",
        "symfony/dependency-injection": "^6.1",
        "symfony/filesystem": "^6.1",
        "symfony/finder": "^6.1",
        "symfony/http-kernel": "^6.1",
        "symfony/security-bundle": "^6.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13",
        "phpstan/phpstan": "^1.8.11",
        "phpstan/phpstan-strict-rules": "^1.4.4",
        "phpunit/php-code-coverage": "^9.2.18"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Cycle\\SymfonyBundle\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
