{
    "name": "mamchyts/cycle-symfony-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Dmitry Mamchyts"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "cycle/annotated": "^3",
        "cycle/migrations": "4.x-dev",
        "cycle/orm": "^2",
        "cycle/schema-builder": "^2",
        "cycle/schema-migrations-generator": "3.x-dev",
        "symfony/config": "^6.1",
        "symfony/dependency-injection": "^6.1",
        "symfony/http-kernel": "^6.1"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "dev-main"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Cycle\\SymfonyBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
